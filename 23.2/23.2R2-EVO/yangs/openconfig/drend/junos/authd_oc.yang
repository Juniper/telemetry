    rpc juniper-netconf-get {
       dr:command-top-of-output "/oc-authd-information";
       dr:command-help "Default <get> rpc";
       output {
          container junos {
              container system {
		  dr:fieldnum 151;
                  container subscriber-management {
		      dr:fieldnum 151;
                      container aaa{
			  dr:fieldnum 151;
                          container authentication-statistics {
                              dr:source "/junos/system/subscriber-management/aaa/authentication-statistics";
			      dr:fieldnum 152;
                              uses authen-stats-group;
                          }
                          container accounting-statistics {
                              dr:source "/junos/system/subscriber-management/aaa/accounting-statistics";
			      dr:fieldnum 153;
                              uses acct-stats-group;
                          }
                          container dynamic-request-statistics {
                              dr:source "/junos/system/subscriber-management/aaa/dynamic-request-statistics";
			      dr:fieldnum 154;
                              uses dynreq-stats-group;
                          }
                          container radius-servers {
                              dr:source "/junos/system/subscriber-management/aaa/radius-servers";
			      dr:fieldnum 155;
                              uses group-authd-radius-server-statistic-list;
                          }
                          container address-assignment-statistics {
			      dr:fieldnum 156;
                              container logical-system-routing-instances {
                              dr:source "/junos/system/subscriber-management/aaa/address-assignment-statistics/logical-system-routing-instances";
			      dr:fieldnum 151;
                              uses group-authd-address-assignment-lsri-list;
                              }
                          }
                          container diameter {
                              dr:fieldnum 157;
                              container clients {
                                  dr:fieldnum 151;
                                  container nasreq {
                                      description "The set of NASREQ client statistics";
                                      dr:source "/junos/system/subscriber-management/aaa/diameter/clients/nasreq";
                                      dr:fieldnum 151;
                                      uses nasreq-stats-group;
                                  }
                                  container gx {
                                      description "The set of PCRF client statistics";
                                      dr:source "/junos/system/subscriber-management/aaa/diameter/clients/gx";
                                      dr:fieldnum 152;
                                      uses pcrf-stats-group;
                                  }
                                  container gy {
                                      description "The set of OCS client statistics";
                                      dr:source "/junos/system/subscriber-management/aaa/diameter/clients/gy";
                                      dr:fieldnum 153;
                                      uses ocs-stats-group;
                                  }
                              }
                          }
                          container nas5g-statistics {
                              description "NAS-5G statistics";
                              dr:source "/junos/system/subscriber-management/aaa/nas5g-statistics";
                              dr:fieldnum 158;
                              uses nas5g-stats-group;
                          }
                      }
                  }
              }
          }
       }
    }
