            dr:source authentication_request_messages_sent;
            dr:fieldnum 51;
        }

        leaf authentication-request-message-failures {
            description "Authentication request processing errors";
            type uint64;
            dr:source authentication_request_message_failures;
            dr:fieldnum 52;
        }

        leaf authentication-request-messages-timeouts {
            description "Authentication requests timed out";
            type uint64;
            dr:source authentication_request_messages_timeouts;
            dr:fieldnum 53;
        }

        leaf authentication-denies-failures {
            description "Authentication requests rejected";
            type uint64;
            dr:source authentication_denies_failures;
            dr:fieldnum 54;
        }

        leaf authentication-grants-received {
            description "Authentication grants received";
            type uint64;
            dr:source authentication_grants_received;
            dr:fieldnum 55;
        }

        leaf authorization-request-messages-sent {
            description "Authorization requests successfully sent";
            type uint64;
            dr:source authorization_request_messages_sent;
            dr:fieldnum 56;
        }

        leaf authorization-request-message-failures {
            description "Authorization request processing errors";
            type uint64;
            dr:source authorization_request_message_failures;
            dr:fieldnum 57;
        }

        leaf authorization-request-messages-timeouts {
            description "Authorization requests timed out";
            type uint64;
            dr:source authorization_request_messages_timeouts;
            dr:fieldnum 58;
        }

        leaf authorization-denies-failures {
            description "Authorization requests rejected";
            type uint64;
            dr:source authorization_denies_failures;
            dr:fieldnum 59;
        }

        leaf authorization-grants-received {
            description "Authorization grants received";
            type uint64;
            dr:source authorization_grants_received;
            dr:fieldnum 60;
        }

        leaf session-terminate-request-messages-sent {
            description "Termination requests successfully sent";
            type uint64;
            dr:source session_terminate_request_messages_sent;
            dr:fieldnum 61;
        }

        leaf session-terminate-request-message-failures {
            description "Termination request processing errors";
            type uint64;
            dr:source session_terminate_request_message_failures;
            dr:fieldnum 62;
        }

        leaf session-terminate-request-messages-timeouts {
            description "Termination requests timed out";
            type uint64;
            dr:source session_terminate_request_messages_timeouts;
            dr:fieldnum 63;
        }

        leaf session-terminate-response-messages-failures {
            description "Termination response processing errors";
            type uint64;
            dr:source session_terminate_response_messages_failures;
            dr:fieldnum 64;
        }

        leaf session-terminate-response-messages-received {
            description "Termination responses received";
            type uint64;
            dr:source session_terminate_response_messages_received;
            dr:fieldnum 65;
        }

        leaf abort-session-requests-received {
            description "Abort session requests received";
            type uint64;
            dr:source abort_session_requests_received;
            dr:fieldnum 66;
        }

        leaf abort-session-response-ack-messages-sent {
            description "Abort session requests responded with ack";
            type uint64;
            dr:source abort_session_response_ack_messages_sent;
            dr:fieldnum 67;
        }

        leaf abort-session-response-nack-messages-sent {
            description "Abort session requests responded with nack";
            type uint64;
            dr:source abort_session_response_nack_messages_sent;
            dr:fieldnum 68;
        }

        leaf abort-session-response-message-failures {
            description "Abort session response failures";
            type uint64;
            dr:source abort_session_response_message_failures;
            dr:fieldnum 69;
        }

        leaf transmit-queue-time-outs {
            description "Total count of messages dropped from the transmit queue due to timeout";
            type uint64;
            dr:source transmit_queue_time_outs;
            dr:fieldnum 70;
        }

        leaf response-parse-errors {
            description "Response messages dropped due to parsing errors";
            type uint64;
            dr:source response_parse_errors;
            dr:fieldnum 71;
        }

        leaf responses-dropped {
            description "Total count of responses dropped";
            type uint64;
            dr:source responses_dropped;
            dr:fieldnum 72;
        }

        leaf outstanding-requests {
            description "Outstanding request messages";
            type uint64;
            dr:source outstanding_requests;
            dr:fieldnum 73;
        }
    }

     grouping nas5g-stats-group {
         description "The set of NAS-5G client statistics";
