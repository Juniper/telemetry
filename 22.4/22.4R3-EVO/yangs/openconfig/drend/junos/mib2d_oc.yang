notification "oc-ifl-del" {
    dr:source-notification-name "ifl-del";

    container interfaces {
        dr:source "/notification/ifl-del/interfaces";
        list interface {
            dr:source "interface";
            dr:fieldnum 151;
            key "name";
            leaf name {
                type string;
                dr:source "interface-name";
                dr:fieldnum 51;
            }
            container subinterfaces {
		dr:source "subinterfaces";
		dr:fieldnum 152;
                list subinterface {
		    dr:source "subinterface";
		    dr:fieldnum 151;
                    key index;
                    leaf index {
                        type uint32;
			dr:source "subunit";
			dr:fieldnum 51;
                    }
                    leaf name {
                        type string;
			dr:source "name";
			dr:fieldnum 52;
                    }
                    leaf snmp_index {
                        type uint32;
			dr:source "snmp_index";
			dr:fieldnum 55;
                    }
                    leaf logical {
                        type uint32;
			dr:source "logical";
			dr:fieldnum 65;
                    }
                }
            }
        }
    }
}

