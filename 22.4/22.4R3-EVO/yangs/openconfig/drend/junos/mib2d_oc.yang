    dr:source-notification-name "ipv4-proxy-arp-change";
    uses ocif:interfaces-top {
      augment "interfaces" {
        dr:source "/notification/ipv4-proxy-arp-change/interfaces";
      }
      augment "interfaces/interface" {
        dr:source "interface";
        dr:fieldnum 151;
      }
      augment "interfaces/interface/name" {
        dr:source "interface-name";
        dr:fieldnum 51;
      }
      augment "interfaces/interface/subinterfaces" {
        dr:source "subinterfaces";
        dr:fieldnum 152;
      }
      augment "interfaces/interface/subinterfaces/subinterface" {
        dr:source "subinterface";
        dr:fieldnum 151;
        uses oc-ip:ipv4-top {
          augment "ipv4" {
            dr:fieldnum 152;
          }
          augment "ipv4/proxy-arp" {
            dr:source "ipv4/proxy-arp";
