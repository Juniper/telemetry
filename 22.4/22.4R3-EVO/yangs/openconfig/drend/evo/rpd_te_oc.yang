  rpc juniper-netconf-get {
    dr:command-top-of-output "/oc-te-information";
    dr:command-full-name "request drend juniper-netconf-get";
    dr:command-help "Default <get> rpc";

    output {
      container network-instances {
        list network-instance {
          key "name";
          dr:source "/rt-instances/rt-instance";
          dr:fieldnum 151;

          leaf name {
            type string;
            dr:source "instance_name";
            dr:fieldnum 51;
          }
          container mpls {
            dr:fieldnum 151;
            container te-global-attributes {
              dr:source "te/globals";
              dr:fieldnum 151;
              uses rend-mpls-te-srlg-top;
              uses rend-mpls-admin-groups-top;
              uses rend-mpls-te-lsp-timers;
            }
            container te-interface-attributes {
              dr:source "te/interfaces";
              dr:fieldnum 152;
              uses rend-te-interface-attributes-top;
            }
            container lsps {
              dr:fieldnum 153;
              container constrained-path {
                dr:fieldnum 151;
                uses rend-explicit-paths-top;
                uses rend-te-tunnels-top;
                uses rend-te-container-tunnels-top;
              }
              container static-lsps {
                dr:fieldnum 152;
                uses rend-static-lsp-top;
              }
            }
          }
        }
      }
    }
  }

